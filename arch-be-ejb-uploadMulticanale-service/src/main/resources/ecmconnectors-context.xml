<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<context:component-scan base-package="it.bmed.arch.uploadMulticanale.be.service.cmis"/>
	<!-- ALFRESCO ECM CONNECTOR -->
	<bean id="alfrescoConnector" class="it.bmed.arch.uploadMulticanale.be.service.cmis.AlfrescoConnector">
		<!-- User credentials -->
		<property name="user" value="up-multi-dev" />
		<property name="password" value="uploadmulti001!" />
		<!-- Binding Settings -->
		<property name="bindingSPIType" value="org.apache.chemistry.opencmis.client.bindings.spi.webservices.CmisWebServicesSpi"/>
		<!-- CMIS WebService Connection Settings -->
		<property name="repositoryServiceUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/RepositoryService?wsdl" />
		<property name="navigationServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/NavigationService?wsdl"/>
		<property name="objectServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/ObjectService?wsdl"/>
		<property name="versioningServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/VersioningService?wsdl" />
		<property name="discoveryServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/DiscoveryService?wsdl"/>
		<property name="relationshipServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/RelationshipService?wsdl" />
		<property name="multiFilingServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/MultiFilingService?wsdl" />
		<property name="policyServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/PolicyService?wsdl"/>
		<property name="aclServiceWsdlUrl" value="https://test-alfresco.mediolanum.it/alfresco/cmisws/cmis/services/ACLService?wsdl"/>
	</bean>
	
	<!-- FILENET ECM CONNECTOR -->
	<bean id="filnetConnector" class="it.bmed.arch.uploadMulticanale.be.service.cmis.FilenetConnector">
		<!-- User credentials -->
		<property name="user" value="" />
		<property name="password" value="" />
		
		<!-- CMIS WebService Connection Settings -->
		<property name="repositoryServiceUrl" value="" />
		<property name="navigationServiceWsdlUrl" value=""/>
		<property name="objectServiceWsdlUrl" value=""/>
		<property name="versioningServiceWsdlUrl" value="" />
		<property name="discoveryServiceWsdlUrl" value=""/>
		<property name="relationshipServiceWsdlUrl" value="" />
		<property name="multiFilingServiceWsdlUrl" value="" />
		<property name="policyServiceWsdlUrl" value=""/>
		<property name="aclServiceWsdlUrl" value=""/>
	</bean>
</beans>
